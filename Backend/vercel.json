{
  "version": 2,
  "builds": [
    {
      "src": "Backend/Server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Backend/Server.js"
    }
  ],
  "env": {
    "PORT": "PORT",
    "MONGODB_URL": "MONGODB_URL",
    "JWT_SECRET": "JWT_SECRET",
    "SMTP_USER": "SMTP_USER",
    "SMTP_PASSWORD": "SMTP_PASSWORD",
    "SENDER_EMAIL": "SENDER_EMAIL"
  }
}
